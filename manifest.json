{
   "author": "proEndreeper, Modified by SSyl to work for UESP",
   "background": {
      "persistent": true,
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_icon": "icon32.png",
      "default_popup": "popup.html",
      "default_title": "UESP Wiki Redirector"
   },
   "description": "Redirects Elder Scrolls Wikia/Fandom requests to UESP",
   "icons": {
      "128": "icon128.png",
      "48": "icon48.png"
   },
   "manifest_version": 2,
   "name": "UESP Wiki Redirector",
   "permissions": [ "storage", "*://elderscrolls.wikia.com/*","*://elderscrolls.fandom.com/*", "webNavigation", "tabs" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0"
}
